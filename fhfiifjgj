samson_svc_password: >-
  {{ lookup(
      'cyberark',
      'safe=IV2AFOCUS-LOGIN'
      ~ ';appid=' ~ appid
      ~ ';credential=' ~ svc_credential
      ~ ';region=' ~ region
      ~ ';field=password'
      ~ ';ca=' ~ cyberark_ca
      ~ ';cert=' ~ cyberark_cert
      ~ ';key=' ~ cyberark_key
  ) }}

# Backward compatibility for role
samson_service_username: "{{ samson_svc_user }}"
svc_user: "{{ samson_svc_user }}